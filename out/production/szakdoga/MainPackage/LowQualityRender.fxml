<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="MainPackage.LowQualityRenderController"
           onMousePressed="#handleMousePress" onMouseDragged="#handleMouseDrag">

    <ImageView fx:id="mrxsView" />

    <VBox alignment="TOP_RIGHT">
        <Button fx:id="exitButton"/>
        <Button fx:id="gridViewButton" onAction="#handleGridViewButton"/>
    </VBox>

</StackPane>
