<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainPackage.ChooserScreenController" fx:id="stackPane">
    <HBox alignment="CENTER" fx:id="hBox">
        <Button text="Single-Picture mode" fx:id="singleButton" onAction="#handleSingleButton"/>
        <Button text="Multiple-Picture mode" fx:id="multipleButton" onAction="#handleMultipleButton"/>
    </HBox>

</StackPane>
