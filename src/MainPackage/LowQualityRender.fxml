<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="MainPackage.LowQualityRenderController"
           onMousePressed="#handleMousePress" onMouseDragged="#handleMouseDrag">

    <ImageView fx:id="mrxsView" />

    <HBox alignment="TOP_RIGHT">
        <Button text="GridView" fx:id="gridViewButton" onAction="#handleGridViewButton"/>
    </HBox>

</StackPane>
